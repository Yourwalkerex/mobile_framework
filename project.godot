config_version=5

[application]

config/name="Familiar Tails Mobile"
run/main_scene="res://b_main/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://c_system_files/icon.svg"

[autoload]

camera_manager="*res://a_autoloads/camera_manager.tscn"
constant="*res://a_autoloads/const.gd"
enumerator="*res://a_autoloads/enum.gd"
level_manager="*res://a_autoloads/level_manager.gd"
prng="*res://a_autoloads/prng.gd"
signal_bus="*res://a_autoloads/signal_bus.gd"
mobile_manager="*res://a_autoloads/mobile_manager.tscn"

[display]

window/size/viewport_width=240
window/size/viewport_height=426
window/size/always_on_top=true
window/stretch/aspect="ignore"

[editor]

